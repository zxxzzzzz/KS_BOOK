<import src="../../../common/head.wxml" />
<import src="../../../common/foot.wxml" />


<view class="container">
  <template is="head" />

  <view class="page-body">
    <view class="page-section">
      <view class="weui-cells weui-cells_after-title">
        <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
            <view class="weui-label">查询座位id</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" type="text" placeholder="请输入查询的座位id" name="deskid" value="{{deskid}}" bindinput="deskidChange"></input>
          </view>
        </view>
        <view class="btn-area">
          <button type="primary" bindtap="confirmSearch">确认查询</button> 
        </view>
      </view>
        <view class="btn-area">
          <button type="primary" bindtap="goBack">返回导航</button> 
        </view>

    </view>
  </view>


  <template is="foot" />
</view>